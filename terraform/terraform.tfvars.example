# AWS Configuration
aws_region = "us-east-1"
environment = "dev"

# VPC Configuration
vpc_cidr = "10.0.0.0/16"
availability_zones = ["us-east-1a", "us-east-1b", "us-east-1c"]

# Database Configuration
database_name = "finco_db"
database_username = "finco_admin"
rds_multi_az = true
rds_instance_class = "db.t3.medium"

# Redis Configuration
redis_node_type = "cache.t3.micro"
redis_num_nodes = 2

# Application Configuration
backend_image = "123456789012.dkr.ecr.us-east-1.amazonaws.com/finco-backend:latest"
frontend_image = "123456789012.dkr.ecr.us-east-1.amazonaws.com/finco-frontend:latest"
backend_cpu = 512
backend_memory = 1024
frontend_cpu = 256
frontend_memory = 512
